{
  "name": "snail-ui",
  "version": "0.0.1",
  "description": "基于vue2.0的移动UI组件库",
  "scripts": {
    "dev": "npm run bootstrap && npm run build:entry",
    "dist": "npm run dist:dev && npm run dist:prod",
    "dist:dev": "webpack --config build/webpack.dist.dev.config.js",
    "dist:prod": "webpack --config build/webpack.dist.prod.config.js",
    "prepublish": "npm run dist",
    "build": "",
    "build:entry": "node build/build.js",
    "build:all": "node build/build.js -a",
    "clean": "rimraf ./dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ct-fed/snail-ui.git"
  },
  "keywords": [
    "vue",
    "ui",
    "vue",
    "snail",
    "snail-ui"
  ],
  "author": "fhuanweb@gmail.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ct-fed/snail-ui/issues"
  },
  "homepage": "https://github.com/ct-fed/snail-ui#readme",
  "dependencies": {
    "vue": "^2.1.10"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.2",
    "axios": "^0.15.3",
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-istanbul": "^3.1.2",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-latest": "^6.22.0",
    "babel-preset-stage-2": "^6.22.0",
    "babel-register": "^6.22.0",
    "babel-runtime": "^6.20.0",
    "chalk": "^1.1.3",
    "css-loader": "^0.26.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.10.0",
    "lerna": "2.0.0-beta.38",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "ora": "^1.1.0",
    "postcss-plugin-px2rem": "^0.6.0",
    "query-string": "^4.3.2",
    "shelljs": "^0.7.6",
    "url-loader": "^0.5.7",
    "vue-loader": "^10.3.0",
    "vue-style-loader": "^2.0.0",
    "vue-template-compiler": "^2.1.10",
    "webpack": "^2.2.1",
    "webpack-merge": "^2.6.1",
    "yargs": "^6.5.0"
  }
}
